{
  "annotations": { "list": [] },
  "description": "API Performance Monitoring â€” Request rates, latency, errors",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "Request Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "id": 2,
      "title": "Request Rate (req/s)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(nginx_http_requests_total[5m]))",
          "legendFormat": "Total Requests/s"
        },
        {
          "expr": "sum(rate(nginx_http_requests_total{status=~\"2..\"}[5m]))",
          "legendFormat": "2xx (Success)"
        },
        {
          "expr": "sum(rate(nginx_http_requests_total{status=~\"4..\"}[5m]))",
          "legendFormat": "4xx (Client Error)"
        },
        {
          "expr": "sum(rate(nginx_http_requests_total{status=~\"5..\"}[5m]))",
          "legendFormat": "5xx (Server Error)"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.05 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 1 },
      "id": 3,
      "title": "Error Rate",
      "type": "stat",
      "options": { "colorMode": "background", "graphMode": "area", "textMode": "auto" },
      "targets": [
        {
          "expr": "sum(rate(nginx_http_requests_total{status=~\"5..\"}[5m])) / sum(rate(nginx_http_requests_total[5m]))",
          "legendFormat": "5xx Error Rate"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 1 },
      "id": 4,
      "title": "Active Connections",
      "type": "stat",
      "options": { "colorMode": "value", "graphMode": "area", "textMode": "auto" },
      "targets": [
        {
          "expr": "nginx_connections_active",
          "legendFormat": "Active"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "id": 10,
      "title": "Response Times",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "id": 11,
      "title": "Backend Response Time (p50 / p95 / p99)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{job=\"laravel-backend\"}[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"laravel-backend\"}[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=\"laravel-backend\"}[5m])) by (le))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "id": 12,
      "title": "Nginx Upstream Response Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(nginx_upstream_response_time_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(nginx_upstream_response_time_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 20,
      "title": "Endpoint Breakdown",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 19 },
      "id": 21,
      "title": "Request Rate by Status Code",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by (status) (rate(nginx_http_requests_total[5m]))",
          "legendFormat": "{{ status }}"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 19 },
      "id": 22,
      "title": "Network Traffic",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(nginx_http_request_bytes_sent[5m]))",
          "legendFormat": "Bytes Sent/s"
        },
        {
          "expr": "sum(rate(nginx_http_request_bytes_received[5m]))",
          "legendFormat": "Bytes Received/s"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
      "id": 30,
      "title": "Queue & Jobs",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 28 },
      "id": 31,
      "title": "Queue Length",
      "type": "timeseries",
      "targets": [
        {
          "expr": "redis_list_length{key=~\".*queue.*\"}",
          "legendFormat": "{{ key }}"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 28 },
      "id": 32,
      "title": "Jobs Processed/s",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(laravel_queue_jobs_processed_total[5m])",
          "legendFormat": "Processed/s"
        },
        {
          "expr": "rate(laravel_queue_jobs_failed_total[5m])",
          "legendFormat": "Failed/s"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 28 },
      "id": 33,
      "title": "Job Processing Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(laravel_queue_job_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95 Job Duration"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["logistics", "api", "performance"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "API Performance",
  "uid": "logistics-api-performance",
  "version": 1
}
